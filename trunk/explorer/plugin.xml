<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
    <extension
        point="org.eclipse.ui.actionSets">
        <actionSet
            label="Explorer Set"
            visible="true"
            id="com.zhengrenqi.explorerfile.actionSet">
            <action
                label="&amp;ExplorerFile"
                icon="icons/folder.gif"
                class="com.zhengrenqi.explorerfile.actions.ExplorerAction"
                tooltip="Explorer"
                toolbarPath="Explorer"
                id="com.zhengrenqi.explorerfile.actions.ExplorerAction">
            </action>
        </actionSet>
    </extension>
    <extension
          point="org.eclipse.ui.startup">
       <startup
             class="com.zhengrenqi.explorerfile.Activator"></startup>
    </extension>
</plugin>

